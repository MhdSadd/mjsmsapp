						<li><a href="/#courses">DMS Courses</a></li>
						<li><a href="/blog">DMS Blog</a></li>
						<li><a href="/contact">Contact</a></li>
						<%if (session) { %>
						<li><a href="/admin">Dashboard</a></li>
						<% } else { %>
						<li><a href="/login">Login</a></li>
						<% } %>
